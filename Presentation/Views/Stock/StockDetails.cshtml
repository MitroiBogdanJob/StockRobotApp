@model StockApp.Core.Stock

@{
    ViewData["Title"] = "Stock Details";
}

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="welcome-message">
    <p>Here is the stock price.</p>
</div>

<h2>@Model.StockName</h2>
<p>Price: @Model.Price</p>
<ul class="stock-exchange-list">
<a href="@Url.Action("StockMenu", "Stock", new { exchangeCode = "LSE" })"> < Back to Stock Menu</a>
</ul>